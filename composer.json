{
    "type": "project",
    "license": "agpl-3",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thomasez/aceeditor-bundle"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ajaxray/short-code": "^1.1",
        "bisonlab/common-bundle": "dev-master",
        "bisonlab/context-bundle": "dev-main",
        "bisonlab/noorm-bundle": "dev-attributes",
        "bisonlab/reports-bundle": "dev-master",
        "bisonlab/sakonnin-bundle": "dev-master",
        "components/jquery": "^3.7",
        "components/jqueryui": "^1.12",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "doctrine/dbal": "^3.5",
        "eluceo/ical": "^0.13.0",
        "fortawesome/font-awesome": "^6.5",
        "moment/moment": "^2.20.1",
        "jms/serializer-bundle": "^5.4",
        "knplabs/knp-menu-bundle": "^3.6",
        "oomphinc/composer-installers-extender": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^2.1",
        "snapappointments/bootstrap-select": "^1.13",
        "symfony/apache-pack": "^1.0",
        "symfony/flex": "^2",
        "symfony/asset": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.1",
        "twbs/bootstrap": "^4.5",
        "twig/extra-bundle": "^3.30",
        "twig/twig": "^3.8",
        "wikimedia/composer-merge-plugin": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.41",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.0",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "7.3.*",
        "symfony/var-dumper": "^6.3",
        "symfony/web-profiler-bundle": "7.3.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "bisonlab/*": "source",
            "norzechowicz/*": "source",
            "vaibhavpandeyvpz/*": "source",
            "*": "dist"
        },
        "component-dir": "public/components",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "rm -rf public/bootstrap",
            "rm -rf public/bootstrap-select",
            "rm -rf var/cache/*",
            "cp -R vendor/twbs/bootstrap/dist public/bootstrap",
            "cp -R vendor/snapappointments/bootstrap-select/dist public/bootstrap-select",
            "rm -rf public/font-awesome",
            "mkdir -p public/font-awesome",
            "cp -R vendor/fortawesome/font-awesome/webfonts public/font-awesome/.",
            "cp -R vendor/fortawesome/font-awesome/css public/font-awesome/."
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        },
        "installer-types": ["component"],
        "installer-paths": {
            "public/components/{$name}/": ["type:component"]
        },
        "merge-plugin": {
            "include": [
                "src/CustomBundle/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    }
}
