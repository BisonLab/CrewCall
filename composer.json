{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thomasez/aceeditor-bundle"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.4.*",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.0",
        "twbs/bootstrap": "^4.5",
        "eluceo/ical": "^0.13.0",
        "fortawesome/font-awesome": "^5.15",
        "fullcalendar/fullcalendar": "^3.10",
        "jms/serializer-bundle": "^3.5",
        "knplabs/knp-markdown-bundle": "^1.8",
        "knplabs/knp-menu-bundle": "^3.0",
        "oomphinc/composer-installers-extender": "dev-master",
        "symfony/apache-pack": "^1.0",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.1",
        "ajaxray/short-code": "^1.1",
        "bisonlab/common-bundle": "dev-no-contexts",
        "bisonlab/context-bundle": "dev-main",
        "bisonlab/noorm-bundle": "dev-master",
        "bisonlab/reports-bundle": "dev-master",
        "bisonlab/sakonnin-bundle": "dev-new-context",
        "components/jquery": "^3.4",
        "components/jqueryui": "^1.12",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "symfony/var-dumper": "^5.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "robloach/component-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "bisonlab/*": "source",
            "norzechowicz/*": "source",
            "vaibhavpandeyvpz/*": "source",
            "*": "dist"
        },
        "component-dir": "public/components",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CustomBundle\\": "src/CustomBundle/",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "rm -rf public/bootstrap",
            "rm -rf var/cache/*",
            "cp -R vendor/twbs/bootstrap/dist public/bootstrap",
            "rm -rf public/font-awesome",
            "mkdir -p public/font-awesome",
            "cp -R vendor/fortawesome/font-awesome/webfonts public/font-awesome/.",
            "cp -R vendor/fortawesome/font-awesome/css public/font-awesome/."
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        },
        "installer-types": ["component"],
        "installer-paths": {
            "public/components/{$name}/": ["type:component"]
        }
    }
}
