{
    "name": "bisonlab/crewcall",
    "license": "Affero GPL v3",
    "type": "project",
    "description": "The CrewCall Base applitcation",
    "homepage": "https://github.com/BisonLab/CrewCall",
    "authors": [
        {
            "name": "Thomas Lundquist",
            "email": "github@bisonlab.no"
        }
    ],
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernelCustomTrait.php", "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thomasez/aceeditor-bundle"
        }
    ],
    "require": {
        "php": "^7.1",
        "ajaxray/short-code": "^1.1",
        "bisonlab/common-bundle": "dev-master",
        "bisonlab/noorm-bundle": "dev-master",
        "bisonlab/reports-bundle": "master@dev",
        "bisonlab/sakonnin-bundle": "dev-master",
        "braincrafted/bootstrap-bundle": "dev-master",
        "commerceguys/addressing": "dev-master",
        "components/jquery": "^3.2",
        "components/jqueryui": "^1.12",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "eluceo/ical": "^0.13.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "fullcalendar/fullcalendar": "^3.8",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/di-extra-bundle": "^1.9",
        "jms/security-extra-bundle": "^1.6",
        "jms/serializer-bundle": "^2.3",
        "knplabs/knp-menu-bundle": "dev-master",
        "norzechowicz/aceeditor-bundle": "dev-dev-thomasez",
        "robloach/component-installer": "dev-master",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/symfony": "3.4.*",
        "twbs/bootstrap": "^3.3",
        "twig/twig": "^2.4",
        "knplabs/knp-markdown-bundle": "^1.7"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ]
    },
    "config": {
        "component-dir": "web/public",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        },
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    }
}
