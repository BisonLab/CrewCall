{% extends 'base.html.twig' %}

{% block body %}
<div class="container">

{% set newrow = true %}
{% for dash in dashboarder.dashboards(app.user) %}
{% if newrow == true %}
<div class="row">
{% set newrow = false %}
{% else %}
{% set newrow = true %}
{% endif %}
<div class="col-sm-6">
{{ dash | raw }}
</div>
{% if newrow == true %}
</div>
{% endif %}
{% endfor %}
{% if newrow == false %}
</div>
{% endif %}

</div>
{% endblock %}
